}
.goodspage {
  height: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0rpx 0rpx 10rpx 2rpx #ebebebde;
  margin: 20rpx 0;
}
.goodimage {
  height: 100rpx;
  width: 100rpx;
  border-radius: 10rpx;
