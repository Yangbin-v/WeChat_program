  z-index: 5;
  flex-direction: column;
  box-shadow: 0rpx 2rpx 10rpx 2rpx #b8b8b8;
}
.ware {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-items: space-between;
  align-items: center;
  margin: 20rpx;
}
.ware_ {
  width: 250rpx;
  height: 30rpx;
  display: flex;
  flex-direction: column;
}
.logpage {
  height: 30rpx;
  display: flex;
  flex-direction: column;
}
.warename {
  font-size: 20rpx;
  font-weight: 400;
}
.outlog {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 20rpx;
}
.inputpage_ {
  width: 400rpx;
  height: 50rpx;
  flex-direction: row;
  justify-items: space-between;
  align-items: center;
